    "@unocss/reset": "^0.57.1",
    "houdini": "^1.2.32",
    "houdini-svelte": "^1.2.32",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "unocss": "0.57.1",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@trpc/client": "^10.43.0",
    "clsx": "^2.0.0",
    "superjson": "^1.12.3"
