{
  "name": "sveltekit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint --ignore-path=../../.eslintignore .",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@iconify/json": "2.2.39",
    "@sveltejs/adapter-auto": "2.0.1",
    "@sveltejs/kit": "1.15.9",
    "@unocss/extractor-svelte": "^0.51.8",
    "@unocss/preset-mini": "^0.51.8",
    "@unocss/reset": "^0.51.8",
    "houdini": "^1.2.3",
    "houdini-svelte": "^1.2.3",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "unocss": "0.51.8",
    "vite": "^4.3.3"
  },
  "dependencies": {
    "@trpc/client": "^10.23.0",
    "clsx": "^1.2.1",
    "superjson": "^1.12.3"
  }
}
